{
  "name": "@minissg/render-svelte",
  "version": "1.0.0",
  "description": "Preact plugin for vite-plugin-minissg",
  "homepage": "https://github.com/uenoB/vite-plugin-minissg#readme",
  "repository": "github:uenoB/vite-plugin-minissg",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "exports": {
    ".": {
      "types": "dist/index.d.ts",
      "import": "dist/index.js",
      "default": "dist/index.cjs"
