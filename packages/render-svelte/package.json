    "./package.json": "./package.json"
  },
  "files": [
    "dist/index.cjs",
    "dist/index.cjs.map",
    "dist/index.d.cts",
    "dist/index.d.ts",
    "dist/index.js",
    "dist/index.js.map"
  ],
  "keywords": [
    "vite-plugin-minissg",
    "svelte"
  ],
  "peerDependencies": {
    "svelte": "^4.0.0",
    "vite-plugin-minissg": "^1.0.0"
  }
}
