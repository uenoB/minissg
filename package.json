      "@minissg/page": "workspace:^",
      "@minissg/render-preact": "workspace:^",
      "@minissg/render-react": "workspace:^",
      "@minissg/render-solid": "workspace:^",
      "@minissg/render-svelte": "workspace:^",
      "@minissg/render-vue": "workspace:^"
    }
