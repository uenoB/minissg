{
  "name": "vite-plugin-minissg",
  "version": "0.1.0",
  "description": "Minimum-sized static site generator as a Vite plugin",
  "homepage": "https://github.com/uenoB/vite-plugin-minissg#readme",
  "repository": "github:uenoB/vite-plugin-minissg",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./renderer/*": {
      "types": "./dist/renderer/*.d.ts",
      "import": "./dist/renderer/*.js",
      "default": "./dist/renderer/*.cjs"
    },
    "./client": {
      "types": "./client.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "client.d.ts",
    "dist/index.js",
    "dist/index.js.map",
    "dist/index.cjs",
    "dist/index.cjs.map",
    "dist/index.d.ts",
    "dist/renderer/*.js",
    "dist/renderer/*.js.map",
    "dist/renderer/*.cjs",
    "dist/renderer/*.cjs.map",
    "dist/renderer/*.d.ts"
  ],
  "keywords": [
    "vite-plugin",
    "ssg",
    "ssr"
  ],
  "scripts": {
    "prepare": "git config --local core.hooksPath .githooks || exit 0",
    "git-pre-commit": "lint-staged",
    "build": "rollup -c rollup.config.js"
  },
  "pnpm": {
    "overrides": {
      "vite-plugin-minissg": "workspace:^"
    }
  },
  "dependencies": {
    "es-module-lexer": "^1.3.0",
    "magic-string": "^0.30.2",
    "mrmime": "^1.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^20.5.0",
    "eslint": "^8.47.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-svelte": "^2.32.4",
    "eslint-plugin-vue": "^9.17.0",
    "lint-staged": "^14.0.0",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.2",
    "prettier-plugin-svelte": "^3.0.3",
    "rollup": "^3.28.0",
    "rollup-plugin-dts": "^5.3.1",
    "semantic-release": "^21.0.7",
    "stylelint": "^15.10.2",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.1"
  },
  "peerDependencies": {
    "vite": ">=4"
  }
}
